{
  "name": "layer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:app": "turbo dev --filter=\"{./apps/app}...\"",
    "dev:web": "turbo dev --filter=\"{./apps/web}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "build:app": "turbo build --filter=\"...{./apps/app}\"",
    "build:web": "turbo build --filter=\"...{./apps/web}\""
  },
  "devDependencies": {
    "turbo": "^2.1.1",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    }
  }
}
